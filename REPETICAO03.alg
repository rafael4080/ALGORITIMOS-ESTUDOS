Algoritmo "semnome"

Var
   valor,contador,sal:Inteiro

Inicio
   contador <-0
   Escreva("Quer contar até quanto ?")
   Leia(valor)
   Escreva("Qual será o salto ?")
   Leia(sal)
   Enquanto (contador<= valor)FACA
      EscrevaL(contador)
      contador <-contador +sal
   FimEnquanto

Fimalgoritmo